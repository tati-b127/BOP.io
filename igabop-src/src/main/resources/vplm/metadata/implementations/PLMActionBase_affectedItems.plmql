NAVIGATE 
    RefECA.PLMID, RefECA.PLM_ExternalID
FROM 
    PLMChgActionBase/PLMActionBase RefECA
FROM_ROOTS [?1?]
ON (RELATION(FROM=PLMChgActionBase/PLMActionBase TO=PLMChgTaskInput/PLMChgTaskInput THROUGH=POINTEDBY) WITH QUALITY_FILTER (CATEGORY=ISOWNEDBY))
THEN ( (RELATION(FROM=PLMChgTaskInput/PLMChgTaskInput TO=PLMCORE/PLMCoreReference THROUGH=POINTSTO)) OR (RELATION(FROM=PLMChgTaskInput/PLMChgTaskInput TO=PLMCORE/PLMCoreInstance THROUGH=POINTSTO)) OR (RELATION(FROM=PLMChgTaskInput/PLMChgTaskInput TO=PLMCORE/PLMCoreRepReference THROUGH=POINTSTO)))
ITER=1;

