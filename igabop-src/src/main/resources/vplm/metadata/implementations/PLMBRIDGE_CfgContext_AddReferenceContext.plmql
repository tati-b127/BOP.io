NAVIGATE
	T0.PLM_ExternalID,
	T0.PLMID,
	T1.PLM_ExternalID,
	T1.PLMID 
FROM
	PLMBRIDGE_CfgContext/PLMCfgContext T0,
	PLMCORE/PLMCoreReference T1
FROM_ROOTS [?1?]
ON ( RELATION(
	FROM=PLMCORE/PLMCoreReference
	TO=PLMBRIDGE_CfgContext/PLMCfgContext 
	THROUGH=POINTEDBY)
WITH QUALITY_FILTER (CATEGORY=ISOWNEDBY))
;
