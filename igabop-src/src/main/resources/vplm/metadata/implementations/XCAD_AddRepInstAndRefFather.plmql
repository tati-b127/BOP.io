NAVIGATE 

T0.PLM_ExternalID,T1.PLM_ExternalID,T2.PLM_ExternalID,T0.PLMID,T1.PLMID,T2.PLMID


FROM PLMCORE/PLMCoreRepReference T0,PLMCORE/PLMCoreRepInstance T1,PLMCORE/PLMCoreReference T2

FROM_ROOTS[?1?]

ON (RELATION(FROM=PLMCORE/PLMCoreRepReference TO=PLMCORE/PLMCoreRepInstance THROUGH=POINTEDBY)
WITH QUALITY_FILTER(CATEGORY=IsInstanceOf))

THEN (RELATION(FROM=PLMCORE/PLMCoreRepInstance TO=PLMCORE/PLMCoreReference THROUGH=POINTSTO)
WITH QUALITY_FILTER(CATEGORY=IsOwnedBy))

ITER=1;
