NAVIGATE 

T0.PLMID, T1.PLMID

FROM PLMCORE/PLMCoreReference T0, PLMCORE/PLMCoreInstance T1 
FROM_ROOTS [?1?] 

ON RELATION(FROM=PLMCORE/PLMCoreReference TO=PLMCORE/PLMCoreInstance THROUGH=POINTEDBY) WITH QUALITY_FILTER (CATEGORY=ISINSTANCEOF) 
THEN RELATION(FROM=PLMCORE/PLMCoreInstance TO=PLMCORE/PLMCoreReference THROUGH=POINTSTO) WITH QUALITY_FILTER (CATEGORY=ISOWNEDBY);
