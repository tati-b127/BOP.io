NAVIGATE
    	T1.PLMID ,			T2.PLMID ,
    	T1.PLM_ExternalID ,	T2.PLM_ExternalID
FROM
	ENOCLG_CLASS/ENOCLG_ClassReference T1,
	ENOCLG_CLASS/ENOCLG_ClassInstance T2
FROM_ROOTS
	[?1?]

ON
	RELATION
		(FROM=ENOCLG_CLASS/ENOCLG_ClassReference
		 TO=ENOCLG_CLASS/ENOCLG_ClassInstance
		 THROUGH=POINTEDBY
		 )
		 WITH QUALITY_FILTER  (CATEGORY=ISINSTANCEOF)
THEN
	RELATION
		(FROM=ENOCLG_CLASS/ENOCLG_ClassInstance
		 TO=ENOCLG_CLASS/ENOCLG_ClassReference
		 THROUGH=POINTSTO
		 )
		 WITH QUALITY_FILTER  (CATEGORY=ISOWNEDBY)
ITER = 10 ;
