NAVIGATE T0.PLM_ExternalID,T1.PLM_ExternalID,T2.PLM_ExternalID,T0.PLMID,T1.PLMID,T2.PLMID
FROM PRODUCTCFG/VPMRepReference T0,PRODUCTCFG/VPMRepInstance T1,PRODUCTCFG/VPMReference T2
FROM_ROOTS[?1?]
ON (RELATION(FROM=PRODUCTCFG/VPMRepReference TO=PRODUCTCFG/VPMRepInstance THROUGH=POINTEDBY)
WITH QUALITY_FILTER(CATEGORY=IsInstanceOf))
THEN (RELATION(FROM=PRODUCTCFG/VPMRepInstance TO=PRODUCTCFG/VPMReference THROUGH=POINTSTO)
WITH QUALITY_FILTER(CATEGORY=IsOwnedBy))
ITER=1;
