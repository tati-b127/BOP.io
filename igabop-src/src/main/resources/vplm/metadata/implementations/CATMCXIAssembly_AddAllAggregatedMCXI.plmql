NAVIGATE   
T0.PLM_ExternalID, T1.PLM_ExternalID,
T0.PLMID, T1.PLMID 

FROM 
CATMCXIAssembly/CATMCXMechanicalConnectionInstance T0,
PRODUCTCFG/VPMReference T1

FROM_ROOTS[?1?]
ON 	(
	RELATION
		(
		FROM=PRODUCTCFG/VPMReference 
		TO=CATMCXIAssembly/CATMCXMechanicalConnectionInstance 
		THROUGH=POINTEDBY
		)
 	WITH QUALITY_FILTER	( CATEGORY=ISOWNEDBY ) 	
	)
; 
